{
  "mcpServers": {
    "mcp-perplexity-search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-perplexity-search"
      ],
      "env": {
        "PERPLEXITY_API_KEY": "$PERPLEXITY_API_KEY"
      }
    },
    "mcp-sequentialthinking-tools": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-sequentialthinking-tools"
      ]
    },
    "mcp-jinaai-search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-jinaai-search"
      ],
      "env": {
        "JINAAI_API_KEY": "$JINAAI_API_KEY"
      }
    },
    "mcp-jinaai-grounding": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-jinaai-grounding"
      ],
      "env": {
        "JINAAI_API_KEY": "$JINAAI_API_KEY"
      }
    },
    "mcp-jinaai-reader": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-jinaai-reader"
      ],
      "env": {
        "JINAAI_API_KEY": "$JINAAI_API_KEY"
      }
    },
    "mcp-tavily-search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-tavily-search"
      ],
      "env": {
        "TAVILY_API_KEY": "$TAVILY_API_KEY"
      }
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-brave-search"
      ],
