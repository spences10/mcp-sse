{
  "mcpServers": {
    "mcp-jinaai-search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-jinaai-search"
      ],
      "env": {
        "JINAAI_API_KEY": "$JINAAI_API_KEY"
      }
    },
    "mcp-tavily-search": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-tavily-search"
      ],
      "env": {
        "TAVILY_API_KEY": "$TAVILY_API_KEY"
      }
    }
  }
}